*.foreground:    #F8F8F8
*.background:    #262626
*.cursorColor:   #F8F8F8
*.highlightColor:#3A3F4B

! Negro y gris
*.color0:        #1E2127
*.color8:        #5C6370
! Rojos
*.color1:        #E06C75
*.color9:        #E06C75
! Verde
*.color2:        #98C379
*.color10:       #98C379
! Amarillo
*.color3:        #D19A66
*.color11:       #D19A66
! Azul
*.color4:        #61AFEF
*.color12:       #61AFEF
! Magenta
*.color5:        #C678DD
*.color13:       #C678DD
! Cyan
*.color6:        #56B6C2
*.color14:       #56B6C2
! Blanco
*.color7:        #ABB2BF
*.color15:       #FFFFFF

! Addons
URxvt.perl-ext-common: default,tabbed, clipboard

! Disabling Yellow Window
URxvt.iso14755: false

! Fonts
URxvt.font: xft:Hack Nerd Font:pixelsize=14:antialias=true
URxvt.boldFont: xft:Hack Nerd Font Bold:pixelsize=14:antialias=true
URxvt.italicFont: xft:Hack Nerd Font Italic:pixelsize=14:antialias=true
URxvt.letterSpace: 0

!Tabs
URxvt.tabbed.tabbar-fg: 5
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    2
URxvt.tabbed.tab-bg:    0


! ScrollBar
URxvt.scrollBar: false

! Cursor Blink
URxvt.cursorBlink: 1
